{
  "name": "browser-fakepty",
  "version": "0.0.1",
  "description": "A fake pty implementation for in-browser xterm.js usage.",
  "main": "./lib/index.js",
  "scripts": {
    "test": "mocha lib/**/*.test.js",
    "tsc": "tsc",
    "watch": "tsc -w",
    "lint": "tslint 'src/**/*.ts'",
    "start": "webpack-cli && http-server",
    "prepublish": "npm run tsc",
    "coverage": "nyc --reporter=lcov --reporter=text --reporter=html npm test",
    "benchmark": "xterm-benchmark $*",
    "build": "tsc -b ./tsconfig.json",
    "preheaders": "npm run build",
    "headers": "node bin/create_headers.js",
    "clean": "rm -rf lib include/*.h"
  },
  "keywords": [
    "pty",
    "browser",
    "pseudoterminal",
    "xterm.js"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "xterm": "^4.4.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.8",
    "@types/minimist": "^1.2.0",
    "@types/mocha": "^7.0.1",
    "chai": "^4.2.0",
    "chalk": "^3.0.0",
    "http-server": "^0.12.0",
    "mocha": "^7.0.1",
    "node-pty": "^0.9.0",
    "node-termios": "^0.0.13",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "xterm-benchmark": "^0.1.3"
  }
}
